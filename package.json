{
  "name": "@bruceshih/eslint-config-vue",
  "type": "module",
  "version": "1.7.0",
  "packageManager": "pnpm@9.2.0",
  "description": "Bruce's eslint config for Vue",
  "author": "Bruce Shih",
  "license": "MIT",
  "homepage": "https://github.com/BruceShih/eslint-config-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BruceShih/eslint-config-vue.git"
  },
  "bugs": {
    "url": "https://github.com/BruceShih/eslint-config-vue/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "eslint-flat-config-viewer",
    "build": "tsup --format esm,cjs --clean --dts",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "test": "vitest"
  },
  "peerDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "eslint": "^9.0.0",
    "eslint-flat-config-utils": "^0.1.2",
    "eslint-plugin-format": "^0.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@bruceshih/eslint-config-vue": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "bumpp": "^9.4.1",
    "eslint-flat-config-utils": "^0.1.2",
    "eslint-flat-config-viewer": "^0.1.20",
    "eslint-plugin-format": "^0.1.1",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "jiti": "^1.21.0",
    "prettier": "^3.3.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "vue": "^3.4.27"
  }
}
