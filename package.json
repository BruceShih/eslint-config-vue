{
  "name": "@bruceshih/eslint-config-vue",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@8.15.5",
  "description": "Bruce's eslint config for Vue",
  "author": "Bruce Shih",
  "license": "MIT",
  "homepage": "https://github.com/BruceShih/eslint-config-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BruceShih/eslint-config-vue.git"
  },
  "bugs": {
    "url": "https://github.com/BruceShih/eslint-config-vue/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "eslint-flat-config-viewer",
    "build": "tsup --format esm,cjs --clean --dts",
    "lint": "eslint .",
    "test": "vitest"
  },
  "peerDependencies": {
    "@antfu/eslint-config": "^2.11.3",
    "eslint": ">=8.40.0",
    "eslint-flat-config-utils": "^0.1.2"
  },
  "devDependencies": {
    "@bruceshih/eslint-config-vue": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "eslint-flat-config-utils": "^0.1.2",
    "eslint-flat-config-viewer": "^0.1.14",
    "eslint-plugin-format": "^0.1.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "jiti": "^1.21.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0",
    "vue": "^3.4.21"
  }
}
